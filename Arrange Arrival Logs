#include <stdio.h>

int main() {
    int n1, n2;
    int a[100], b[100], merged[200];

    printf("Enter number of entries in first log: ");
    scanf("%d", &n1);

    printf("Enter %d arrival times (sorted):\n", n1);
    for (int i = 0; i < n1; i++) {
        scanf("%d", &a[i]);
    }

    printf("Enter number of entries in second log: ");
    scanf("%d", &n2);

    printf("Enter %d arrival times (sorted):\n", n2);
    for (int i = 0; i < n2; i++) {
        scanf("%d", &b[i]);
    }

    int i = 0, j = 0, k = 0;

    // Merge the two logs
    while (i < n1 && j < n2) {
        if (a[i] <= b[j]) {
            merged[k++] = a[i++];
        } else {
            merged[k++] = b[j++];
        }
    }

    // Copy remaining elements
    while (i < n1) {
        merged[k++] = a[i++];
    }

    while (j < n2) {
        merged[k++] = b[j++];
    }

    printf("Merged chronological log:\n");
    for (int x = 0; x < k; x++) {
        printf("%d ", merged[x]);
    }
